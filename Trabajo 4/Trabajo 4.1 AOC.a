.data

dato:	.ascii "F0000005"
#Utilizar sb y lb

.text
.globl __start
__start:
addi $s0,$zero,8
la $a0,dato
jal Función

Función:
j Binario

Binario:
lb $s1,0($a0)
slti $s2,$s1,58
beq $s2,1,Numero
beq $s2,0,Letra

Numero:
addi $s1,$s1,-48
or $v0,$s1,$v0
addi $s0,$s0,-1
beq $s0,0,Fin
rol $v0,$v0,4
addi $a0,$a0,1
jr $ra

Letra:
addi $s1,$s1,-55
or $v0,$s1,$v0
addi $s0,$s0,-1
beq $s0,0,Fin
rol $v0,$v0,4
addi $a0,$a0,1
jr $ra

Fin:
